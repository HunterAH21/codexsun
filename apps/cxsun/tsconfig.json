{
  "compilerOptions": {
    "noEmit": true,
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "types": ["node"],
    "jsx": "react"
  },
  "include": ["app.ts","src", "vite.config.ts", "**/*.tsx"],
  "types": ["vite/client"],
}
